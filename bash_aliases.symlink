# Git
alias gp="git push"
alias gl="git pull"
alias gs="git status"
alias ga="git add --all"
alias gm="git commit -m"

# Ls
alias la="ls -A"
alias ll='ls -alF'
alias l='ls -CF'

alias alert='notify-send --urgency=low -i "$([ $? = 0 ] && echo terminal || echo error)" "$(history|tail -n1|sed -e '\''s/^\s*[0-9]\+\s*//;s/[;&|]\s*alert$//'\'')"'
